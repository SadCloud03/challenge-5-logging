{
    "version" : 1,
    "disable_existing_loggers" : false,
    "formatters" : {
        "simple" : {
            "format" : "%(asctime)s | %(levelname)s | %(name)s | %(message)s"
        }
    },
    "handlers" : {
        "stderr" : {
            "class" : "logging.StreamHandler",
            "level" : "WARNING",
            "formatter" : "simple",
            "stream" : "ext://sys.stderr"
        },
        "file": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "formatter": "simple",
            "filename": "logs/logsTest.log",
            "maxBytes": 10000,
            "backupCount": 3
        }

    },
    "loggers" : {
        "root" : {
            "level" : "DEBUG",
            "handlers" : [
                "stderr",
                "file"
            ]
        }
    }
}